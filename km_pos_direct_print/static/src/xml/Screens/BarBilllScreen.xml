<templates id="template">
    <t t-name="BarBillScreenExtend" t-inherit="km_pos_slip_kitchen_bar.BarBillScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[@class='button print']" position="replace">
            <div class="button print" t-on-click="sendReceiptToPrintNode">
                <i class="fa fa-print"></i>
                <span> </span>
                <span> Print </span>
            </div>
        </xpath>
    </t>
</templates>