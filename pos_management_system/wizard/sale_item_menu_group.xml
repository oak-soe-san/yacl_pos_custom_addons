<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_item_menu_group_report_wizard_form" model="ir.ui.view">
        <field name="name">sale.item.menu.group.report.wizard.form</field>
        <field name="model">sale.item.menu.group.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Sale Iteam Menu Group Wizard Report">
                <sheet>
                    <group>
                        <group>
                            <field name="date_from" />
                            <field name="date_to" />
                        </group>
                    </group>
                    <footer>
                        <button name="action_print_report" type="object" string="Print PDF"
                            class="btn-primary" />
                        <button name="action_print_excel_report" type="object" string="Print Excel"
                            class="btn-primary" />
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

</odoo>